/** common.wxss **/
page {
  color: #353535;
  font-family: "Helvetica Neue", Helvetica, Arial, "PingFang SC", "Hiragino Sans GB", "Heiti SC", "Microsoft YaHei", "WenQuanYi Micro Hei", sans-serif;
  background: #f5f5f5;
  width: 100%;
  height: 100%;
}
.disabled-btn {
  width: 716rpx;
  height: 94rpx;
  margin: 0 auto;
  text-align: center;
  line-height: 94rpx;
  border-radius: 10rpx;
  border: none;
  background: #ccc;
  color: #FFF;
}
.active-btn {
  background: #078dff;
}
.pos-rel {
  position: relative;
}
.flex {
  display: flex;
  align-items: center;
}
.none {
  display: none;
}
image {
  display: block;
}